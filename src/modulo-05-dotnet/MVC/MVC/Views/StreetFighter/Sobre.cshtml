@model SobreMimModel

@{
    ViewBag.Title = "Sobre";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<header class="subtitulo"> @Model.Nome </header>
<img src="@Model.UrlImagem" class="img-about">
<p style="padding-top: 50px;">
    <strong>Idade:</strong> @Model.Idade<br>
    <strong>Origem:</strong> @Model.Origem<br>
    <strong>Escolaridade:</strong> @Model.Escolaridade<br>
    <strong>Contato:</strong> @Model.Contato<br>
